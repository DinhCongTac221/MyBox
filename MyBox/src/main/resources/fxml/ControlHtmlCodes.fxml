<?import javafx.scene.web.*?>
<?import mara.mybox.fxml.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>

<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ControlHtmlCodes">
    <children>
        <FlowPane alignment="TOP_RIGHT" hgap="5.0" prefWrapLength="800.0" vgap="5.0" VBox.vgrow="NEVER">
            <children>
            <CheckBox fx:id="wrapCheck" mnemonicParsing="false" text="%Wrap" />
            <Button fx:id="editButton" layoutX="477.0" layoutY="10.0" mnemonicParsing="false" onAction="#editAction">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconEdit.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
                <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clearAction">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconClear.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            <Button fx:id="pasteTxtButton" layoutX="1087.0" layoutY="10.0" mnemonicParsing="false" onAction="#pasteTxt">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconPaste.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
                <Button fx:id="codesButton" mnemonicParsing="false" onMouseEntered="#popCodesMenu">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconMeta.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="headerButton" mnemonicParsing="false" onMouseEntered="#popHeaderMenu">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconHeader.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="listButton" mnemonicParsing="false" onMouseEntered="#popListMenu">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconList.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="addTableButton" mnemonicParsing="false" onAction="#addTable">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconSplit.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="addImageButton" mnemonicParsing="false" onAction="#addImage">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconPicSmall.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="linkButton" mnemonicParsing="false" onAction="#addlink">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconLink.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="pButton" mnemonicParsing="false" onAction="#addP">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconP.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            <Button fx:id="charButton" layoutX="965.0" layoutY="10.0" mnemonicParsing="false" onMouseEntered="#popCharMenu">
               <graphic>
                  <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../buttons/Red/iconCharacter.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
                <Button fx:id="brButton" mnemonicParsing="false" onAction="#addBr">
                    <graphic>
                        <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconBr.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </FlowPane>
        <TextArea fx:id="codesArea" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" wrapText="true" VBox.vgrow="ALWAYS" />
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</VBox>
