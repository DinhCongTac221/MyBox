<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.effect.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<BorderPane fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.WebElementsController">
    <top>
        <fx:include fx:id="mainMenu" source="MainMenu.fxml" />
    </top>
    <center>
        <VBox spacing="5.0">
            <children>
                <HBox fx:id="addressBox" alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="NEVER">
                    <children>
                        <Button fx:id="selectFileButton" mnemonicParsing="false" onAction="#selectSourceFile" onMouseEntered="#popSourceFile">
                            <graphic>
                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconOpen.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <ComboBox fx:id="urlBox" editable="true" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                        <Button fx:id="goButton" defaultButton="true" mnemonicParsing="false" onAction="#goAction">
                            <graphic>
                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconGo.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="functionsButton" mnemonicParsing="false" onMouseEntered="#popFunctionsMenu">
                            <graphic>
                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconFunction.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </HBox>
                <HBox fx:id="elementsBox" alignment="CENTER_LEFT" spacing="5.0">
                    <children>
                        <RadioButton fx:id="tagRadio" layoutX="716.0" layoutY="19.0" minHeight="-Infinity" mnemonicParsing="false" selected="true" text="%Tag" wrapText="true">
                            <toggleGroup>
                                <ToggleGroup fx:id="elementGroup" />
                            </toggleGroup>
                        </RadioButton>
                        <RadioButton fx:id="idRadio" minHeight="-Infinity" mnemonicParsing="false" text="%ID" toggleGroup="$elementGroup" wrapText="true" />
                        <RadioButton fx:id="nameRadio" layoutX="716.0" layoutY="19.0" minHeight="-Infinity" mnemonicParsing="false" text="%Name" toggleGroup="$elementGroup" wrapText="true" />
                        <fx:include fx:id="elementInput" maxHeight="1.7976931348623157E308" source="ControlStringSelector.fxml"  HBox.hgrow="ALWAYS" />
                        <Button fx:id="queryElementButton" mnemonicParsing="false" onAction="#queryElement">
                            <graphic>
                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconQuery.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="recoverButton" layoutX="817.0" layoutY="10.0" mnemonicParsing="false" onAction="#recoverAction">
                            <graphic>
                                <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/iconRecover.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button> 
                    </children>
                </HBox>
                <WebView fx:id="webView" minHeight="200.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                <Label fx:id="bottomLabel" maxWidth="1.7976931348623157E308" minHeight="-Infinity" wrapText="true" VBox.vgrow="NEVER" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>

    </center>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</BorderPane>
