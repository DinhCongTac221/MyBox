<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainPane" alignment="CENTER" prefHeight="600.0" prefWidth="700.0" spacing="2.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.Data2DOperateController">
    <children>
        <SplitPane fx:id="splitPane" dividerPositions="0.6" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
            <items>
                <VBox fx:id="mainBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0">
                    <children>
                        <HBox alignment="CENTER_LEFT" maxWidth="1.7976931348623157E308" spacing="2.0" VBox.vgrow="NEVER">
                            <children>
                                <ImageView fx:id="tipsView" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/Red/iconTips.png" />
                                    </image>
                                </ImageView>
                                <Label fx:id="dataNameLabel" maxWidth="1.7976931348623157E308" minHeight="-Infinity" wrapText="true" HBox.hgrow="ALWAYS" />
                            </children>
                        </HBox>
                        <TitledPane fx:id="sourcePane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%DataSource" VBox.vgrow="ALWAYS">
                            <content>
                                <fx:include fx:id="source" source="ControlData2DSource.fxml" />
                            </content>
                        </TitledPane>
                        <TitledPane fx:id="resultPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%Result" VBox.vgrow="ALWAYS">
                            <content>
                                <fx:include fx:id="result" source="ControlData2DResult.fxml" />
                            </content>
                        </TitledPane>
                    </children>
                </VBox>
                <ScrollPane fx:id="rightPane" fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true">
                    <content>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <children>
                                <Accordion fx:id="accordionPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" VBox.vgrow="ALWAYS">
                                    <panes> 
                                        <TitledPane fx:id="setValuePane" animated="false" text="%SetValues">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox fx:id="setValuesBox">
                                                            <children>
                                                                <fx:include fx:id="setValues" source="ControlData2DSetValues.fxml" />
                                                            </children>
                                                            <padding>
                                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                            </padding>
                                                        </VBox>
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="setValuePane1" animated="false" layoutX="10.0" layoutY="10.0" text="%Normalize">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox spacing="10.0">
                                                            <children>
                                                                <Label text="%Object" />
                                                                <FlowPane alignment="CENTER_LEFT" hgap="5.0" prefWrapLength="200.0" vgap="5.0">
                                                                    <children>
                                                                        <RadioButton mnemonicParsing="false" selected="true" text="%Columns">
                                                                            <toggleGroup>
                                                                                <ToggleGroup fx:id="nObjectGroup" />
                                                                            </toggleGroup>
                                                                        </RadioButton>
                                                                        <RadioButton mnemonicParsing="false" text="%Rows" toggleGroup="$nObjectGroup" />
                                                                        <RadioButton mnemonicParsing="false" text="%Matrix" toggleGroup="$nObjectGroup" />
                                                                    </children>
                                                                </FlowPane>
                                                                <Label text="%Algorithm" />
                                                                <RadioButton mnemonicParsing="false" selected="true" text="Min/Max">
                                                                    <toggleGroup>
                                                                        <ToggleGroup fx:id="nAlgorithmGroup" />
                                                                    </toggleGroup>
                                                                </RadioButton>
                                                                <RadioButton mnemonicParsing="false" text="Z-Score" toggleGroup="$nAlgorithmGroup" />
                                                                <RadioButton mnemonicParsing="false" text="L2-Regularization" toggleGroup="$nAlgorithmGroup" />
                                                                <RadioButton mnemonicParsing="false" text="L1-Regularization" toggleGroup="$nAlgorithmGroup" />
                                                            </children>
                                                            <padding>
                                                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                                            </padding>
                                                        </VBox>
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="copyPane" animated="false" text="%Copy">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox fx:id="targetBox" spacing="10.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
                                                            <children>
                                                                <Label text="%Target" />
                                                                <RadioButton fx:id="csvRadio" mnemonicParsing="false" selected="true" text="%NewData">
                                                                    <toggleGroup>
                                                                        <ToggleGroup fx:id="targetGroup" />
                                                                    </toggleGroup>
                                                                </RadioButton>
                                                                <VBox fx:id="tableBox" spacing="2.0">
                                                                    <children>
                                                                        <Label text="%InTable" />
                                                                        <FlowPane alignment="CENTER_LEFT" hgap="2.0" prefWrapLength="200.0" vgap="2.0">
                                                                            <children>
                                                                                <RadioButton fx:id="frontRadio" mnemonicParsing="false" text="%Front" toggleGroup="$targetGroup" />
                                                                                <RadioButton fx:id="endRadio" mnemonicParsing="false" text="%End" toggleGroup="$targetGroup" />
                                                                                <RadioButton fx:id="aboveRadio" mnemonicParsing="false" text="%Above" toggleGroup="$targetGroup" />
                                                                                <RadioButton fx:id="belowRadio" mnemonicParsing="false" text="%Below" toggleGroup="$targetGroup" />
                                                                            </children>
                                                                        </FlowPane>
                                                                        <Label text="%TableRow" />
                                                                        <ComboBox fx:id="rowSelector" prefWidth="150.0" />
                                                                    </children>
                                                                </VBox>
                                                            </children>
                                                        </VBox>
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="statisticPane" animated="false" layoutX="10.0" layoutY="10.0" text="%Statistic">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox spacing="2.0" />
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="percentagePane" animated="false" layoutX="10.0" layoutY="34.0" text="%Percentage">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox spacing="2.0" />
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="transposePane" animated="false" layoutX="10.0" layoutY="10.0" text="%Transpose">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox spacing="2.0" />
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                        <TitledPane fx:id="sortPane" animated="false" layoutX="10.0" layoutY="104.0" text="%Sort">
                                            <content>
                                                <ScrollPane fitToHeight="true" fitToWidth="true" pannable="true">
                                                    <content>
                                                        <VBox spacing="2.0" />
                                                    </content>
                                                </ScrollPane>
                                            </content>
                                        </TitledPane>
                                    </panes>
                                </Accordion>
                            </children>
                        </VBox>
                    </content>
                </ScrollPane>
            </items>
        </SplitPane>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
</VBox>
