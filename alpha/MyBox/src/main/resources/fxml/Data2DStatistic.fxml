<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" prefHeight="600.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.Data2DStatisticController">
    <children>
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <children>
                <Label fx:id="titleLabel" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="%Statistic" textAlignment="CENTER" VBox.vgrow="NEVER" />
            </children>
        </HBox>
      <fx:include fx:id="source" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="ControlData2DSource.fxml" VBox.vgrow="ALWAYS" />
          <Label minHeight="-Infinity" text="%NonnumericCountAsZero" wrapText="true" />
        <VBox fx:id="operationBox" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
            <children>
                <HBox alignment="CENTER_LEFT" spacing="2.0">
                    <children>
                        <Label minHeight="-Infinity" minWidth="-Infinity" text="%Operations" />
                        <Button fx:id="selectAllButton" mnemonicParsing="false" onAction="#selectAllAction">
                            <graphic>
                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/Red/iconSelectAll.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                        <Button fx:id="selectNoneButton" mnemonicParsing="false" onAction="#selectNoneAction">
                            <graphic>
                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../buttons/Red/iconSelectNone.png" />
                                    </image>
                                </ImageView>
                            </graphic>
                        </Button>
                    </children>
                </HBox>
                <FlowPane hgap="2.0" maxWidth="1.7976931348623157E308" minHeight="-Infinity" prefWrapLength="300.0" vgap="2.0">
                    <children>
                        <CheckBox fx:id="countCheck" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%Count" />
                        <CheckBox fx:id="summationCheck" layoutX="70.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%Summation" />
                        <CheckBox fx:id="meanCheck" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%Mean" />
                        <CheckBox fx:id="maximumCheck" layoutX="10.0" layoutY="29.0" mnemonicParsing="false" text="%Maximum" />
                        <CheckBox fx:id="minimumCheck" layoutX="89.0" layoutY="29.0" mnemonicParsing="false" text="%Minimum" />
                  <CheckBox fx:id="geometricMeanCheck" layoutX="181.0" layoutY="10.0" mnemonicParsing="false" text="%GeometricMean" />
                  <CheckBox fx:id="sumOfSquaresCheck" layoutX="358.0" layoutY="10.0" mnemonicParsing="false" text="%SumOfSquares" />
                        <CheckBox fx:id="populationVarianceCheck" layoutX="136.0" layoutY="10.0" mnemonicParsing="false" text="%PopulationVariance" />
                  <CheckBox fx:id="sampleVarianceCheck" layoutX="238.0" layoutY="10.0" mnemonicParsing="false" text="%SampleVariance" />
                  <CheckBox fx:id="populationStandardDeviationCheck" layoutX="238.0" layoutY="10.0" mnemonicParsing="false" text="%PopulationStandardDeviation" />
                  <CheckBox fx:id="sampleStandardDeviationCheck" layoutX="286.0" layoutY="10.0" mnemonicParsing="false" text="%SampleStandardDeviation" />
                        <CheckBox fx:id="skewnessCheck" layoutX="446.0" layoutY="10.0" mnemonicParsing="false" text="%Skewness" />
                        <CheckBox fx:id="modeCheck" layoutX="372.0" layoutY="10.0" mnemonicParsing="false" text="%Mode" />
                        <CheckBox fx:id="medianCheck" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" text="%Median" />
                    </children>
                </FlowPane>
                <Label fx:id="memoryNoticeLabel" minHeight="-Infinity" styleClass="dark-blue-label" stylesheets="@../styles/MyBox.css" text="%DataStatisticComments" wrapText="true" />
            </children>
        </VBox>
        <VBox fx:id="targetBox" spacing="5.0" styleClass="focusBox" stylesheets="@../styles/MyBox.css">
            <children>
                <fx:include fx:id="target" source="ControlData2DTarget.fxml" />
            </children>
        </VBox>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <children>
                <Label fx:id="infoLabel" maxWidth="1.7976931348623157E308" styleClass="dark-blue-label" stylesheets="@../styles/MyBox.css" HBox.hgrow="ALWAYS" />
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconCancel.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="okButton" mnemonicParsing="false" onAction="#okAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconOK.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
