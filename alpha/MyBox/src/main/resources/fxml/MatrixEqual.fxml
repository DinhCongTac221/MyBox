<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" spacing="10.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.DataEqualController">
    <children>
        <HBox alignment="CENTER" VBox.vgrow="NEVER">
            <children>
                <Label fx:id="titleLabel" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" text="%SetValues" textAlignment="CENTER" VBox.vgrow="NEVER" />
            </children>
        </HBox>
        <Label fx:id="targetLabel" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="%Target" textAlignment="CENTER" wrapText="true" />
        <HBox fx:id="codeBox1" layoutX="30.0" layoutY="55.0" spacing="5.0">
            <children>
                <Label minHeight="-Infinity" minWidth="-Infinity" text="%Rows" />
                <RadioButton fx:id="rowCheckedRadio" layoutX="27.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%Selected">
                    <toggleGroup>
                        <ToggleGroup fx:id="rowGroup" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="rowCurrentPageRadio" mnemonicParsing="false" text="%CurrentPage" toggleGroup="$rowGroup" />
                <RadioButton fx:id="rowAllRadio" mnemonicParsing="false" text="%AllPages" toggleGroup="$rowGroup" />
                <RadioButton fx:id="rowSelectRadio" layoutX="77.0" layoutY="14.0" mnemonicParsing="false" text="%Select" toggleGroup="$rowGroup" />
                <fx:include fx:id="rowsList" prefHeight="150.0" prefWidth="100.0" source="ControlCheckBoxList.fxml" />
            </children>
        </HBox>
        <HBox fx:id="codeBox" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <Label minHeight="-Infinity" minWidth="-Infinity" text="%Columns" />
                <RadioButton fx:id="colCheckedRadio" layoutX="27.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%Selected">
                    <toggleGroup>
                        <ToggleGroup fx:id="colGroup" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="colAllRadio" mnemonicParsing="false" text="%All" toggleGroup="$colGroup" />
                <RadioButton fx:id="colSelectRadio" layoutX="77.0" layoutY="14.0" mnemonicParsing="false" text="%Select" toggleGroup="$colGroup" />
                <fx:include fx:id="colsList" prefHeight="150.0" prefWidth="200.0" source="ControlCheckBoxList.fxml" />
            </children>
        </HBox>
        <FlowPane hgap="5.0" vgap="5.0">
            <children>
                <Label text="%Value" />
                <RadioButton fx:id="zeroRadio" mnemonicParsing="false" selected="true" text="0">
                    <toggleGroup>
                        <ToggleGroup fx:id="valueGroup" />
                    </toggleGroup>
                </RadioButton>
                <RadioButton fx:id="oneRadio" mnemonicParsing="false" text="1" toggleGroup="$valueGroup" />
                <RadioButton fx:id="blankRadio" mnemonicParsing="false" text="%Blank" toggleGroup="$valueGroup" />
                <RadioButton fx:id="randomRadio" layoutX="94.0" layoutY="14.0" mnemonicParsing="false" text="%Random" toggleGroup="$valueGroup" />
                <RadioButton fx:id="setRadio" mnemonicParsing="false" text="%Set" toggleGroup="$valueGroup" />
                <TextField fx:id="valueInput" prefWidth="100.0" />
            </children>
        </FlowPane>
        <HBox alignment="CENTER_RIGHT" spacing="5.0">
            <children>
                <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconCancel.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="refreshButton" layoutX="338.0" layoutY="10.0" mnemonicParsing="false" onAction="#refreshAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconRefresh.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button fx:id="okButton" mnemonicParsing="false" onAction="#okAction">
                    <graphic>
                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                            <image>
                                <Image url="@../buttons/Red/iconOK.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
