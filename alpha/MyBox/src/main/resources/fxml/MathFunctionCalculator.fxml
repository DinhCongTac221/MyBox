<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="thisPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.MathFunctionCalculator">
    <children>
        <TabPane fx:id="tabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <tabs>
                <Tab fx:id="definitionTab" text="%Definition">
                    <content>
                        <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true">
                            <content>
                                <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                                    <children>
                              <Label text="%MathFunctionCalculatorComments" />
                              <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="15.0" spacing="2.0">
                                 <children>
                                    <Label text="%Title" wrapText="true" />
                                    <TextField fx:id="titleInput" disable="true" editable="false" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" spacing="2.0">
                                 <children>
                                    <Label text="%VariablesSeparatedByComma" wrapText="true" />
                                    <TextField fx:id="variablesInput" disable="true" editable="false" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" spacing="2.0">
                                 <children>
                                    <Label maxWidth="1.7976931348623157E308" text="%Expression" wrapText="true" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="recoverScriptButton" mnemonicParsing="false" onAction="#recoverScript">
                                       <graphic>
                                          <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../buttons/Red/iconRecover.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <TextArea fx:id="scriptArea" maxHeight="1.7976931348623157E308" wrapText="true" VBox.vgrow="ALWAYS" />
                              <HBox alignment="CENTER_LEFT" layoutX="15.0" layoutY="91.0" nodeOrientation="LEFT_TO_RIGHT" spacing="2.0">
                                 <children>
                                    <Label maxWidth="1.7976931348623157E308" text="%FunctionDomain" wrapText="true" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="recoverDomainButton" mnemonicParsing="false" onAction="#recoverDomain">
                                       <graphic>
                                          <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../buttons/Red/iconRecover.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                              <TextArea fx:id="domainArea" layoutX="15.0" layoutY="55.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="100.0" prefHeight="200.0" wrapText="true" />
                              <HBox alignment="CENTER_LEFT" spacing="2.0">
                                 <children>
                                    <Label text="%ResultName" wrapText="true" />
                                    <TextField fx:id="resultNameInput" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                    <Button fx:id="recoverNameButton" mnemonicParsing="false" onAction="#recoverResultName">
                                       <graphic>
                                          <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../buttons/Red/iconRecover.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                 </children>
                              </HBox>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                    </padding>
                                </VBox>
                            </content>
                        </ScrollPane>
                    </content>
                </Tab>
                <Tab fx:id="calculateTab" text="%Calculate">
                    <content>
                  <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" pannable="true">
                     <content>
                              <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0">
                                  <children>
                              <FlowPane fx:id="inputsPane" hgap="2.0" layoutX="12.0" layoutY="14.0" minHeight="-Infinity" vgap="2.0" />
                              <FlowPane alignment="CENTER_LEFT" hgap="2.0" prefWrapLength="200.0" vgap="2.0">
                                 <children>
                                    <Label text="%DecimalScale" wrapText="true" />
                                    <ComboBox fx:id="calculateScaleSelector" editable="true" prefWidth="100.0" />
                                 </children>
                              </FlowPane>
                              <HBox alignment="CENTER_LEFT" spacing="2.0">
                                 <children>
                                    <Button fx:id="okCalculateButton" mnemonicParsing="false" onAction="#calculateAction">
                                       <graphic>
                                          <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                             <image>
                                                <Image url="@../buttons/Red/iconOK.png" />
                                             </image>
                                          </ImageView>
                                       </graphic>
                                    </Button>
                                    <Label maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
                                              <Button fx:id="clearResultsButton" mnemonicParsing="false" onAction="#clearResults">
                                                  <graphic>
                                                      <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                          <image>
                                                              <Image url="@../buttons/Red/iconClear.png" />
                                                          </image>
                                                      </ImageView>
                                                  </graphic>
                                              </Button>
                                              <Button fx:id="editResultsButton" mnemonicParsing="false" onAction="#editResults">
                                                  <graphic>
                                                      <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                          <image>
                                                              <Image url="@../buttons/Red/iconEdit.png" />
                                                          </image>
                                                      </ImageView>
                                                  </graphic>
                                              </Button>
                                              <Button fx:id="styleButton" mnemonicParsing="false" onMouseEntered="#popHtmlStyle">
                                                  <graphic>
                                                      <ImageView fitHeight="20" fitWidth="20" pickOnBounds="true" preserveRatio="true">
                                                          <image>
                                                              <Image url="@../buttons/Red/iconStyle.png" />
                                                          </image>
                                                      </ImageView>
                                                  </graphic>
                                              </Button>
                                 </children>
                              </HBox>
                                      <fx:include fx:id="output" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="200.0" source="ControlWebView.fxml" VBox.vgrow="ALWAYS" />
                                  </children>
                                  <padding>
                                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                                  </padding>
                              </VBox>
                     </content>
                  </ScrollPane>
                    </content>
                </Tab>
                <Tab fx:id="dataTab" text="%DataSet">
                    <content>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="2.0">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                            </padding>
                            <children>
                                <TabPane fx:id="dataTabPane" minHeight="-Infinity" tabClosingPolicy="UNAVAILABLE" />
                                <FlowPane alignment="CENTER_LEFT" hgap="2.0" prefWrapLength="200.0" vgap="2.0">
                                    <children>
                                        <Label text="%DecimalScale" wrapText="true" />
                                        <ComboBox fx:id="dataScaleSelector" editable="true" prefWidth="100.0" />
                                    </children>
                                </FlowPane>
                                <Button fx:id="okDataButton" mnemonicParsing="false" onAction="#okDataAction">
                                    <graphic>
                                        <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                            <image>
                                                <Image url="@../buttons/Red/iconOK.png" />
                                            </image>
                                        </ImageView>
                                    </graphic>
                                </Button>
                                <fx:include fx:id="data" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="ControlData2DResults.fxml" VBox.vgrow="ALWAYS" />
                            </children>
                        </VBox>
                    </content>
                </Tab>
                <Tab fx:id="chartTab" text="%Chart">
                    <content>
                        <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                            </padding>
                            <children>
                                <TabPane fx:id="chartTabPane" minHeight="-Infinity" tabClosingPolicy="UNAVAILABLE" />
                                <FlowPane alignment="CENTER_LEFT" hgap="2.0" prefWrapLength="200.0" vgap="2.0">
                                    <children>
                                        <Label text="%DecimalScale" wrapText="true" />
                                        <ComboBox fx:id="chartScaleSelector" editable="true" prefWidth="100.0" />
                                    </children>
                                </FlowPane>
                                <FlowPane fx:id="typesPane" alignment="CENTER_LEFT" hgap="2.0" maxWidth="1.7976931348623157E308" vgap="2.0">
                                    <children>
                                        <RadioButton fx:id="scatterChartRadio" layoutX="51.0" layoutY="10.0" mnemonicParsing="false" selected="true" text="%ScatterChart">
                                            <toggleGroup>
                                                <ToggleGroup fx:id="chartGroup" />
                                            </toggleGroup>
                                        </RadioButton>
                                        <RadioButton fx:id="lineChartRadio" mnemonicParsing="false" text="%LineChart" toggleGroup="$chartGroup" />
                                        <Button fx:id="okChartButton" mnemonicParsing="false" onAction="#okChartAction">
                                            <graphic>
                                                <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
                                                    <image>
                                                        <Image url="@../buttons/Red/iconOK.png" />
                                                    </image>
                                                </ImageView>
                                            </graphic>
                                        </Button>
                                    </children>
                                </FlowPane>
                                <fx:include fx:id="chart" maxWidth="1.7976931348623157E308" source="ControlData2DChartXY.fxml" VBox.vgrow="ALWAYS" />
                            </children>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </children>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
    </padding>
</VBox>
