<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.control.CheckBox?>



<VBox fx:id="thisPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" 
      prefHeight="500.0" prefWidth="600.0"  spacing="2.0"
      xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mara.mybox.controller.ColorsPopController">
    <children>
        <FlowPane alignment="CENTER_RIGHT">
            <children>
                <CheckBox fx:id="onlyNewCheck" mnemonicParsing="false" text="%OnlyAddNewWhenPick"  wrapText="true"  minHeight="-Infinity"/>
            </children>
        </FlowPane>
        <fx:include fx:id="colors" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="ControlColors.fxml" VBox.vgrow="ALWAYS" />
    </children>
    <padding>
        <Insets left="5.0"  top="5.0" />
    </padding>
    <BorderPane.margin>
        <Insets />
    </BorderPane.margin>
</VBox>
