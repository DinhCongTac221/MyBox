                            </children>
                            <padding>
                                <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
                            </padding>
                        </VBox>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
        <HBox fx:id="paginationBox" alignment="CENTER_LEFT" spacing="2.0">
