                                        <CheckBox fx:id="textRowCheck" mnemonicParsing="false" text="%RowNumber" />
                                    </children>
                                </FlowPane>
                                <SplitPane dividerPositions="0.25" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
