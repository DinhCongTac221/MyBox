            <CheckBox fx:id="rightClickPopMenuCheck" mnemonicParsing="false" selected="true" text="%PopMenuWhenMouseRightClickRowCol" />
            <HBox alignment="CENTER_LEFT" spacing="2.0">
               <children>
                  <Label minWidth="-Infinity" text="%SheetWarnThreshold" />
                  <TextField fx:id="warnThresholdInput" prefWidth="120.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <fx:include fx:id="editDelimiter" source="ControlTextDelimiter.fxml" />
